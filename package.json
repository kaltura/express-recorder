{
  "name": "express-recorder",
  "version": "1.0.22",
  "license": "AGPL-3.0",
  "homepage": "https://github.com/kaltura/playkit-js-hotspots#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kaltura/playkit-js-hotspots.git"
  },
  "description": "",
  "main": "dist/express-recorder.js",
  "scripts": {
    "test": "NODE_ENV=test jest",
    "clean": "rimraf dist",
    "reset": "npm run clean && rimraf node_modules",
    "build": "npm run clean && webpack --mode production",
    "serve": "webpack-dev-server -d",
    "analyze": "npm run build && npx source-map-explorer dist/express-recorder.js",
    "storybook": "start-storybook -p 9001 -c .storybook -s ./src/public/assets",
    "storybook-build": "build-storybook -c .storybook -s ./src/public/assets"
  },
  "keywords": [],
  "author": "",
  "devDependencies": {
    "@storybook/react": "^4.1.1",
    "@types/jest": "^23.3.10",
    "awesome-typescript-loader": "^5.2.1",
    "copy-webpack-plugin": "^5.0.2",
    "css-loader": "^2.1.1",
    "dotenv": "^8.0.0",
    "file-loader": "^4.1.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.3.1",
    "jest": "^21.2.1",
    "lint-staged": "^8.1.5",
    "node-sass": "^4.5.3",
    "npm": "^6.9.0",
    "preact-svg-loader": "^0.2.1",
    "prettier": "^1.15.3",
    "rimraf": "^2.6.3",
    "sass-loader": "^7.1.0",
    "source-map-loader": "^0.2.4",
    "style-loader": "^0.23.1",
    "ts-jest": "^23.10.5",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.17.0",
    "tslint-consistent-codestyle": "^1.14.1",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-react": "^3.6.0",
    "typescript": "^3.2.1",
    "url-loader": "^1.1.2",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.2.1"
  },
  "dependencies": {
    "preact": "^8.4.2",
    "kaltura-typescript-client": "file:libs/kaltura-typescript-client-6.4.2-v20181223-153243.tgz"
  },
  "files": [
    "dist"
  ],
  "lint-staged": {
    "*.{ts,tsx,json,css,html}": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "jest": {
    "verbose": true,
    "transform": {
      "^.+\\.js$": "babel-jest"
    },
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "testEnvironment": "node",
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|scss|less)$": "identity-obj-proxy",
      "^react$": "preact-compat",
      "^react-dom$": "preact-compat"
    },
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    },
    "moduleDirectories": [
      "node_modules",
      "src"
    ]
  }
}
